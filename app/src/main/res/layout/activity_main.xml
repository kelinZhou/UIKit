<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical"
    android:gravity="center">

    <Button
        android:id="@+id/btnCommonDemo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="启动一个新的页面" />

    <Button
        android:id="@+id/btnCommonDemo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="启动一个新的页面并传值" />

    <Button
        android:id="@+id/btnTabDemo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="启动一个Tab页面" />

    <Button
        android:id="@+id/btnTabDemo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="启动一个沉浸式Tab页面" />

    <Button
        android:id="@+id/btnTabDemo3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Banner左滑显示更多" />

    <Button
        android:id="@+id/btnTabDemo4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="启动一个搜索页面" />

    <Button
        android:id="@+id/btnTabDemo5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="启动一个沉浸式搜索页面" />

    <Button
        android:id="@+id/btnTabDemo6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="通过浏览器启动H5页面" />

</LinearLayout>